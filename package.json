{
  "name": "geshot",
  "version": "1.1.4",
  "description": " gestion hôtelière",
  "main": "index.js",
  "scripts": {
    "postinstall": "bower install",
    "electron": "electron .",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=all --out=build",
