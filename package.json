  "dependencies": {
    "bluebird": "^3.5.0",
    "bower": "^1.8.2",
    "commander": "^2.11.0",
    "electron-oauth2": "^3.0.0",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.2.1",
    "localStorage": "^1.0.3",
    "materialize-css": "^0.100.2",
    "mkdirp": "^0.5.1",
    "request": "^2.83.0",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.0.3"
  },
  "devDependencies": {
    "electron": "^1.7.6",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^9.1.0"
